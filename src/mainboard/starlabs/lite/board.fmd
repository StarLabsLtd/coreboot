#
# Manually defined FMD in order to ensure that space is reserved for the EC
# at the top of the BIOS region.
#
FLASH 8M {
	SI_DESC@0x0 0x1000
	IFWI 0x320000
	FMAP 0x800
	COREBOOT(CBFS) 0x2E0000
	UNIFIED_MRC_CACHE 0x21000 {
		RECOVERY_MRC_CACHE 0x10000
		RW_MRC_CACHE 0x10000
		RW_VAR_MRC_CACHE 0x1000
	}
	BIOS_UNUSABLE 0x40000
	DEVICE_EXTENSION 0x7f000
	UNUSED_HOLE 0x1000
}

